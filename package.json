{
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^6.2.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "config": "^3.3.6",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "client": "webpack serve --config ./config/webpack.config.dev.js --open --mode development",
    "server": "nodemon --exec babel-node src/server/server.js",
    "dev": "NODE_ENV=development concurrently \"npm run client\" \"npm run server\"",
    "build": "webpack --config ./config/webpack.config.js --mode development",
    "start": "NODE_ENV=production yarn run build && yarn run server",
    "precommit": "lint-staged",
    "lint": "eslint src --fix -c .eslintrc.json 'src/**/*.js'"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
